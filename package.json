{
  "name": "roadmap",
  "version": "0.1.0",
  "description": "your app description",
  "author": "Odekunle Oluwafemi <michael@stackonly.com>",
  "private": true,
  "main": "src/electron-starter.js",
  "dependencies": {
    "@digitalpersona/devices": "^0.2.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.9.1",
    "axios": "^0.21.1",
    "electron-builder-squirrel-windows": "^22.10.5",
    "js-file-download": "^0.4.12",
    "node-sass": "^4.14.1",
    "react": "^17.0.1",
    "react-charts": "^2.0.0-beta.7",
    "react-detect-offline": "^2.4.2",
    "react-dom": "^17.0.1",
    "react-iframe": "^1.8.0",
    "react-minimal-pie-chart": "^8.1.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "web-vitals": "^0.2.4"
  },
  "build": {
    "files": [
      "./build/**/*",
      "./src/electron-starter.js"
    ],
    "appId": "com.frsc.roadmap",
    "extraMetadata": {
      "main": "src/electron-starter.js"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist-all": "electron-builder -mw",
    "prepare-digi": "prepare-digi.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1"
  }
}
